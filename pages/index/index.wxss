page {
  width: 100%;
  height: 100%;
  background: #f0f0f0;
}
.page {
  padding: 15px;
  padding-bottom: 60px;
}
.h2 {
  font-size: 18px;
  margin-bottom: 15px;
}
.list {
  background:#fff;
  padding:15px;
  margin-bottom: 15px;
}
/**
 * #cdcdcd
 * #36ab60
 * #d81e06
 */
.item {
  display: flex;
  align-items:center;
  margin-bottom: 15px;
  padding: 5px;
}
.item-0 {
  border: 1px #36ab60 solid;
  color:#36ab60;
}
.item-1 {
  border: 1px #d81e06 solid;
  color:#d81e06;
}
.item-2 {
  border: 1px #cdcdcd solid;
  color:#cdcdcd;
}
.item:last-child {
  margin-bottom: 0px;
}

.item-icon {
  width: 24px;
}
.item-content {
  flex: 1;
  margin-left: 15px;
}
.car-title {
  font-size: 16px;
}
.car-desc {
  font-size: 14px;
}

.search {
  position: fixed;
  width: 100%;
  bottom: 15px;
}

.search-btn {
  width: 240px;
}